    <connection>db_intelligence_layer</connection>
    <sql>SELECT 
  NVL(ST.CLIENT_ID, S.CLIENT_ID) CLIENT_ID ,
  NVL(ST.ARTICLE_ID, S.ARTICLE_ID) ARTICLE_ID,
  NVL(ST.BOP_DATE_ID, S.DATE_ID) BOP_DATE_ID,
  SUM(NVL(ST.BOP_AMOUNT_ON_STOCK,0)) BOP_AMOUNT_ON_STOCK,
  SUM(NVL(ST.BOP_VALUE_ON_STOCK,0)) BOP_VALUE_ON_STOCK,
  SUM(NVL(ST.BOP_COGS,0)) BOP_COGS,
  SUM(NVL(
    CASE
      WHEN A.ACTIVATION_STATE = 1
      THEN ST.BOP_AMOUNT_ON_STOCK
      ELSE 0
    END,0)
  ) AS BOP_AMOUNT_ON_STOCK_ACT ,
  SUM(NVL(
    CASE
      WHEN A.ACTIVATION_STATE = 1
      THEN ST.BOP_VALUE_ON_STOCK
      ELSE 0
    END,0)
  ) AS BOP_VALUE_ON_STOCK_ACT ,
  SUM(NVL(
    CASE
      WHEN A.ACTIVATION_STATE = 1
      THEN ST.BOP_COGS
      ELSE 0
    END,0)
  ) AS BOP_COGS_ACT ,
  SUM(NVL(PHYSICAL_AMOUNT_ON_STOCK,0)) PHYSICAL_AMOUNT_ON_STOCK,
  SUM(NVL(PHYSICAL_ON_STOCK_COGS,0)) PHYSICAL_ON_STOCK_COGS,
  SUM(NVL(VALUE_PHYSICAL_ON_STOCK,0)) VALUE_PHYSICAL_ON_STOCK,
  NVL(ST.EOP_DATE_ID,S.DATE_ID +1) EOP_DATE_ID,
  SUM(NVL(ST.EOP_AMOUNT_ON_STOCK,0)) EOP_AMOUNT_ON_STOCK,
  SUM(NVL(ST.EOP_VALUE_ON_STOCK,0)) EOP_VALUE_ON_STOCK,
  SUM(NVL(ST.EOP_COGS,0)) EOP_COGS,
  SUM(NVL(
    CASE
      WHEN A.ACTIVATION_STATE = 1
      THEN ST.EOP_AMOUNT_ON_STOCK
      ELSE 0
    END,0)
  ) AS EOP_AMOUNT_ON_STOCK_ACT ,
  SUM(NVL(
    CASE
      WHEN A.ACTIVATION_STATE = 1
      THEN ST.EOP_VALUE_ON_STOCK
      ELSE 0
    END,0)
  ) AS EOP_VALUE_ON_STOCK_ACT ,
  SUM(NVL(
    CASE
      WHEN A.ACTIVATION_STATE = 1
      THEN ST.EOP_COGS
      ELSE 0
    END,0)
  ) AS EOP_COGS_ACT ,
  SUM(COALESCE(S.SHIPPED_ARTICLES, 0)) SHIPPED_ARTICLES ,
  SUM(COALESCE(S.SHIPPED_COGS, 0)) SHIPPED_COGS ,
  SUM(COALESCE(S.RETURNED_ARTICLES, 0)) RETURNED_ARTICLES ,
  SUM(COALESCE(S.RETURNED_COGS, 0)) RETURNED_COGS
FROM STOCKS_LEGACY ST
JOIN ARTICLES A
ON  ST.CLIENT_ID   = A.CLIENT_ID
AND ST.ARTICLE_ID = A.ARTICLE_ID
FULL OUTER JOIN SHIPMENTS S
ON  ST.CLIENT_ID    = S.CLIENT_ID
AND ST.SKU         = S.SKU
AND ST.BOP_DATE_ID = S.DATE_ID
--limit on max bop_date_id in stocks_legacy to avoid unneeded entries for shipments
WHERE S.DATE_ID &lt;= (SELECT MAX(BOP_DATE_ID) FROM STOCKS_LEGACY)
GROUP BY 
  NVL(ST.CLIENT_ID, S.CLIENT_ID),
  NVL(ST.ARTICLE_ID, S.ARTICLE_ID),
  NVL(ST.BOP_DATE_ID, S.DATE_ID),
  NVL(ST.EOP_DATE_ID,S.DATE_ID +1)</sql>
