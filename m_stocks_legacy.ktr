  <step>
    <name>DG_BOB_STOCK_EXPORT</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>db_data_layer</connection>
    <sql>SELECT
  ${var_Client_ID} CLIENT_ID,
  ARTICLE_ID,
  BOP_DATES.DATE_ID AS BOP_DATE_ID,
  EOP_DATES.DATE_ID AS EOP_DATE_ID,
  BOP_AMOUNT_ON_STOCK,
  BOP_AMOUNT_ON_STOCK * PRICE AS BOP_VALUE_ON_STOCK,
  BOP_AMOUNT_ON_STOCK * UNIT_COST AS BOP_COGS,
  PHYSICAL_STOCK PHYSICAL_AMOUNT_ON_STOCK,
  PHYSICAL_STOCK * PRICE AS VALUE_PHYSICAL_ON_STOCK,
  PHYSICAL_STOCK * UNIT_COST AS PHYSICAL_ON_STOCK_COGS,
  EOP_AMOUNT_ON_STOCK,
  EOP_AMOUNT_ON_STOCK * PRICE AS EOP_VALUE_ON_STOCK,
  EOP_AMOUNT_ON_STOCK * UNIT_COST AS EOP_COGS,
  SKU
FROM (
  SELECT 
    ARTICLE_ID,
    TO_CHAR(STOCK_DATE, &apos;YYYY-MM-DD&apos;) BOP_DATE ,
    QUANTITY BOP_AMOUNT_ON_STOCK ,
	PHYSICAL_STOCK,
    COALESCE(LEAD(TO_CHAR(STOCK_DATE, &apos;YYYY-MM-DD&apos;)) OVER (PARTITION BY STOCKS.SKU ORDER BY STOCK_DATE), TO_CHAR(STOCK_DATE, &apos;YYYY-MM-DD&apos;)) EOP_DATE ,
    COALESCE(LEAD(QUANTITY) OVER (PARTITION BY STOCKS.SKU ORDER BY STOCK_DATE), QUANTITY) EOP_AMOUNT_ON_STOCK ,
    UNIT_COST,
    PRICE,
    STOCKS.SKU
  FROM DG_BOB_STOCK_EXPORT_LEGACY STOCKS
  INNER JOIN ${DB_USER_IL}.V_ARTICLES_C ARTICLES
  ON (CLIENT_ID = ${var_Client_ID}
  AND ARTICLES.SKU = STOCKS.SKU
  AND ARTICLES.VALID_FROM &lt;= STOCK_DATE
  AND ARTICLES.VALID_TO &gt; STOCK_DATE)
  WHERE QUANTITY IS NOT NULL
  and 2=2
  and z=z
)
--dates will be joined for following minus
INNER JOIN ${DB_USER_IL}.DATES BOP_DATES
ON (BOP_DATE = BOP_DATES.ISO_DATE)
INNER JOIN ${DB_USER_IL}.DATES EOP_DATES
ON (EOP_DATE = EOP_DATES.ISO_DATE)</sql>
