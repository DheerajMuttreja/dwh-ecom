    <entry>
      <name>Move legacy stock data</name>
      <description/>
      <type>SQL</type>
      <sql>--create temporary table for moving the stock data
CREATE TABLE DG_BOB_STOCK_EXPORT_MOVE AS SELECT * FROM DG_BOB_STOCK_EXPORT WHERE DG_END_DATE &gt; SYSDATE;
--insert legacy data into legacy table
--legacy data is the start of the ISO Week (IW) of the month (MM) 180 days ago
INSERT INTO DG_BOB_STOCK_EXPORT_LEGACY SELECT * FROM DG_BOB_STOCK_EXPORT_MOVE WHERE STOCK_DATE &lt; TRUNC(TRUNC(SYSDATE-180,&apos;MM&apos;),&apos;IW&apos;);</sql>
