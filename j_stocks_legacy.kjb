      <yloc>205</yloc>
      </entry>
    <entry>
      <name>Move current stock data</name>
      <description/>
      <type>SQL</type>
      <sql>--truncate stock table and copy the current data back into the table
TRUNCATE TABLE DG_BOB_STOCK_EXPORT;
INSERT INTO DG_BOB_STOCK_EXPORT SELECT * FROM DG_BOB_STOCK_EXPORT_MOVE WHERE STOCK_DATE &gt;= TRUNC(TRUNC(SYSDATE-180,&apos;MM&apos;),&apos;IW&apos;);
--drop temp table again
DROP TABLE DG_BOB_STOCK_EXPORT_MOVE;</sql>
